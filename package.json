{
  "name": "phoenix-pterodactyl-theme",
  "version": "1.0.0",
  "description": "Phoenix Theme for Pterodactyl Panel by GusmanikCraft",
  "main": "index.js",
  "scripts": {
    "build": "yarn run build:sass && yarn run build:js",
    "build:sass": "sass --style=compressed resources/styles/phoenix.scss public/themes/phoenix/css/phoenix.css",
    "build:js": "webpack --config webpack.config.js",
    "watch": "yarn run watch:sass",
    "watch:sass": "sass --watch resources/styles/phoenix.scss public/themes/phoenix/css/phoenix.css"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "tailwindcss": "^3.3.0",
    "sass": "^1.60.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^4.10.0",
    "terser-webpack-plugin": "^5.3.9"
